package dp.Singleton;

public class SingletonDesignPattern {
	
	
	int Id;
	
	private SingletonDesignPattern() {
		System.out.println("This is singleton.");
	}
	
	static SingletonDesignPattern instance;
	
	public static SingletonDesignPattern createSingletonObject() {
		
		if(instance==null) {
			instance = new SingletonDesignPattern();
		}
		
		return instance;
	}
	

	public static void main(String[] args) {
		
		SingletonDesignPattern singleton = new SingletonDesignPattern();
		System.out.println(singleton);
		
		SingletonDesignPattern singleton1 = new SingletonDesignPattern();
		System.out.println(singleton1);

	}

}
